@article{Savonije_Weaver_Role_Tryptophan_2023,
X_TooBib = {date: 2023},
X_TooBib = {year: 2023,  infield_fix_dates },
X_TooBib = {urldate: FixBeKvp s= cmd=date "+%Y-%m-%d" d=2023-09-25 dn=urldate},
X_TooBib = {author: Savonije , Karl and Weaver , Donald F.},
abbrvjrnl = {Brain Sciences},
abstract = {<jats:p>The need to identify new potentially druggable biochemical mechanisms for Alzheimer's disease (AD) is an ongoing priority. The therapeutic limitations of amyloid-based approaches are further motivating this search. Amino acid metabolism, particularly tryptophan metabolism, has the potential to emerge as a leading candidate and an alternative exploitable biomolecular target. Multiple avenues support this contention. Tryptophan (trp) and its associated metabolites are able to inhibit various enzymes participating in the biosynthesis of upbeta-amyloid, and one metabolite, 3-hydroxyanthranilate, is able to directly inhibit neurotoxic upbeta-amyloid oligomerization; however, whilst certain trp metabolites are neuroprotectant, other metabolites, such as quinolinic acid, are directly toxic to neurons and may themselves contribute to AD progression. Trp metabolites also have the ability to influence microglia and associated cytokines in order to modulate the neuroinflammatory and neuroimmune factors which trigger pro-inflammatory cytotoxicity in AD. Finally, trp and various metabolites, including melatonin, are regulators of sleep, with disorders of sleep being an important risk factor for the development of AD. Thus, the involvement of trp biochemistry in AD is multifactorial and offers a plethora of druggable targets in the continuing quest for AD therapeutics.</jats:p>},
